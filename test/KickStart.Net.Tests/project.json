{
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "KickStart.Net": {
      "target": "project"
    }
  },
  "testRunner": "nunit",
  "buildOptions": {
    "keyFile": "../../kickstart.snk"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": "portable-net45+win8",
      "dependencies": {
        "NUnit": "3.5.0",
		"dotnet-test-nunit": "3.4.0-beta-2",
        "System.Threading.Thread": "4.3.0",
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        }
      },
      "buildOptions": {
        "define": [ "NET_CORE" ]
      }
    },
    "net4.5": {
      "dependencies": {
        "NUnit": "2.*"
      },
      "frameworkAssemblies": {
        "System.Configuration": "4.0.0",
        "System.ComponentModel": "4.0.0"
      }
    }
  }
}